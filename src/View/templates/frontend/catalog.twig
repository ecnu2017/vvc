{% extends 'base.twig' %}

{% block title %}虚拟宠物医院学习系统 - 病例学习{% endblock %}

{% block stylesheets %}
<link rel="stylesheet" type="text/css" href="/frontend/css/char.css">
{% endblock %}

{% block content %}
<div class="navibar">
  <span class='navi-text'>您选择的是，病例学习。</span>
  <div role="button" class='back-text' onclick="window.history.back()">
    <span>返回上一页</span>
  </div>
</div>
<div class='select-bar'>
  <span >请选择病例种类:</span>
</div>
<div class='operations' id='3d-navi'>
  {% for class, ills in catalog %}
  <div role="button" class='bingli' onclick="window.location.href = '/catalog/{{class}}?page=1'">
    <span>{{ class }}</span>
  </div>
  {% endfor %}
</div>
{% endblock %}
